<%= @league.name %>
<br>
<table id="overview">
  <thead>
    <th>Radiant</th>
    <th>Dire</th>
  </thead>
  <tbody>
  <% @league_matches.each do |match| %>
    <tr>
      <td><%= match.radiant_team ? match.radiant_team.name : "Radiant" %></td>
      <td><%= match.dire_team ? match.dire_team.name : "Dire" %></td>
    </tr>
    <tr>
        <%= render partial: "simple_side", locals: {match: match, startIndex: 0, endIndex: 4} %>
        <%= render partial: "simple_side", locals: {match: match, startIndex: 128, endIndex: 132} %>
      <% end %>
    </tr>
  </tbody>
</table>