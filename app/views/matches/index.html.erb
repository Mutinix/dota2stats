<%= @league.name %>
<br>
<% @league_matches.each do |match| %>
<%= match.radiant_team ? match.radiant_team.name : "Radiant" %>
vs
<%= match.dire_team ? match.dire_team.name : "Dire" %>
<br>
<table id="radiant">
  <%= render partial: "side", locals: {match: match, startIndex: 0, endIndex: 4} %>
</table>
<table id="dire">
  <%= render partial: "side", locals: {match: match, startIndex: 128, endIndex: 132} %>
</table>
<% end %>