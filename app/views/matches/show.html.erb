<div id="match">
  <h1 id="victor">
    <% if @match.radiant_win %>
      Radiant Victory!
    <% else %>
      Dire Victory!
    <% end %>
  </h1>

  <table id="radiant">
    <%= render partial: "side", locals: {match: @match, startIndex: 0, endIndex: 4} %>
  </table>
  <table id="dire">
    <%= render partial: "side", locals: {match: @match, startIndex: 5, endIndex: -1} %>
  </table>
</div>